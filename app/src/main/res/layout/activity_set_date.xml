<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.cs2340.team6.totallyratticalapp.SetDateActivity">

    <Button
        android:id="@+id/button11"
        android:layout_width="88dp"
        android:layout_height="46dp"
        android:layout_marginTop="104dp"
        android:onClick="onViewDataPressed"
        android:text="Graph"
        app:layout_constraintHorizontal_bias="0.362"
        app:layout_constraintRight_toLeftOf="@+id/WelcomeText"
        app:layout_constraintTop_toBottomOf="@+id/setDate"
        android:layout_marginLeft="4dp"
        app:layout_constraintLeft_toLeftOf="@+id/setDate" />

    <Spinner
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toBottomOf="parent"
        android:id="@+id/stDaySpinner"
        android:layout_marginTop="15dp"
        />
    <Spinner
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toBottomOf="parent"
        app:layout_constraintLeft_toRightOf="@+id/stDaySpinner"
        android:id="@+id/stMonthSpinner"
        android:layout_marginTop="15dp"
        />
    <Spinner
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toBottomOf="parent"
        app:layout_constraintLeft_toRightOf="@+id/stMonthSpinner"
        android:id="@+id/stYearSpinner"
        android:layout_marginTop="15dp"
        />
    <Spinner
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toBottomOf="@id/stDaySpinner"
        android:id="@+id/eDaySpinner"
        android:layout_marginTop="15dp"
        />
    <Spinner
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toBottomOf="@+id/stMonthSpinner"
        app:layout_constraintLeft_toRightOf="@+id/eDaySpinner"
        android:id="@+id/eMonthSpinner"
        android:layout_marginTop="15dp"
        />
    <Spinner
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toBottomOf="@+id/stYearSpinner"
        app:layout_constraintLeft_toRightOf="@+id/eMonthSpinner"
        android:id="@+id/eYearSpinner"
        android:layout_marginTop="15dp"
        />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toBottomOf="parent"
        android:layout_marginTop="80dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        android:id="@+id/setDate"
        android:text="Set Dates"
        android:onClick="onSetDatePressed"/>

</android.support.constraint.ConstraintLayout>
